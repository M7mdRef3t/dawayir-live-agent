# =============================================================================
# Dawayir Live Agent - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values:
#   cp .env.example .env
# =============================================================================

# Required: Your Gemini API Key (from https://aistudio.google.com/apikey)
GEMINI_API_KEY=

# Required: Your Google Cloud Project ID
GOOGLE_PROJECT_ID=

# Required: GCS Bucket name for mental maps & session reports
GOOGLE_CLOUD_STORAGE_BUCKET=

# Optional: Gemini model (default shown)
GEMINI_LIVE_MODEL=gemini-2.5-flash-native-audio-preview-12-2025

# Optional: API version (default shown)
GEMINI_API_VERSION=v1alpha

# Optional: Server port (default: 8080)
PORT=8080

# Optional: Logging level (info | debug)
LOG_LEVEL=info

# Optional: Authentication token to secure the WebSocket connection
DAWAYIR_AUTH_TOKEN=
